{
    "arrowParens": "always",
    "endOfLine": "lf",
    "tabWidth": 4,
    "printWidth": 120,
    "quoteProps": "consistent",
    "plugins": ["prettier-plugin-tailwindcss"],
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 2,
                "useTabs": false
            }
        }
    ]
}
